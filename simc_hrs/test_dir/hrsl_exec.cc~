#include <stdio.h>
#include <iostream>

using namespace std;

extern "C" void my_subrt_(float *x,float *y,float *z, float *ans); 

int main()
{
  cout << "test of execution..." << endl; 

  float X(3.),Y(4.),Z(0.),Ans(0.);
    
  my_subrt_(&X, &Y, &Z, &Ans); 

  printf("Answer: %5.2f", Ans); cout << endl;  
  
  return 0; 
}

  
